	package test.master;
	
	import java.util.ArrayList;
import java.util.List;

import lejos.robotics.subsumption.Arbitrator;
import lejos.robotics.subsumption.Behavior;
	
	public class MainMaster {
	
	public static void main(String[] args) {
		
	ModelMaster m = new ModelMaster();
	
	//behaviors in this expedition
	Behavior AvoidEdge = new AvoidEdge(m);
	Behavior AvoidCollision = new AvoidCollision(m);
	Behavior FindBlue = new FindBlue(m);
	
	//mission "AvoidStuff"
	List<Behavior> avoidStuff = new ArrayList<>();
	avoidStuff.add(AvoidEdge);
	avoidStuff.add(AvoidCollision);
	Arbitrator arbyAvoidStuff = new Arbitrator((Behavior[])avoidStuff.toArray());
	arbyAvoidStuff.go();
	
	}
}
	
