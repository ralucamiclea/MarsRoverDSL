	package test.master;
	import test.slave.GetMessage;
	import java.util.List;
	import java.util.ArrayList;
	import lejos.robotics.subsumption.Arbitrator;
	import lejos.robotics.subsumption.Behavior;

	
	public class MainMaster {
	
	public static void main(String[] args) {
		
	ModelMaster m = new ModelMaster();
	NXTConnection c;
	GetMessage t = new GetMessage(c,m);
	t.start();
	
	//behaviors in this expedition
	Behavior AvoidEdge = new AvoidEdge(m);
	Behavior AvoidCollision = new AvoidCollision(m);
	Behavior FindBlue = new FindBlue(m);
	
	//mission "AvoidStuff"
	List<Behavior> bArrayAvoidStuff = new ArrayList<>();
	bArrayAvoidStuff.add(AvoidEdge);
	bArrayAvoidStuff.add(AvoidCollision);
	Arbitrator arbyAvoidStuff = new Arbitrator((Behavior[])bArrayAvoidStuff.toArray());
	arbyAvoidStuff.go();
	
	}
}
	
